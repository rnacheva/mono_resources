    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#double">nan</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/arco/ontology/location/AdvancedSpeechCharacteristics">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>AdvancedSpeechCharacteristics</rdfs:label>
    <rdfs:comment>If synthesized speech is produced, the
  user can adjust all of the speech characteristics provided by the speech
  synthesizer Level AAA</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/arco/ontology/location/Standardization">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
