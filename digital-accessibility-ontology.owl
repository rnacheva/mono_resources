    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Multi-ColumnTextReflow</rdfs:label>
    <rdfs:comment>The user can specify that recognized multi-column text blocks each be reflowed into a single column  Level AA</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/arco/ontology/location/ThreeFlashes">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>ThreeFlashes</rdfs:label>
    <rdfs:comment>Web pages do not contain anything that flashes more than three times in any one second period</rdfs:comment>
