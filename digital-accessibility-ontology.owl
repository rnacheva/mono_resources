    <rdfs:label>Answeringmachinefacilityinrelayedcallstoprimaryusers</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#double">nan</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/arco/ontology/location/Tactile input Pointing devices">
